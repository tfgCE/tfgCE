<library group="ui canvas fonts">
	<generateFontTexture>
		<texture file="assets/main very large.tex"
			format="rgba8" wrap="clamp" filteringMag="linear" filteringMin="linear"
			_fontFile="=v:/fonts/VT323-Regular.ttf"
			_fontFile="=v:/fonts/Data-latin.ttf"
			_fontFile="=v:/fonts/Bitwise.ttf"
			_fontFile="=v:/fonts/MadcatRegular-g834.ttf"
			_fontFile="=v:/fonts/zekton rg.ttf"
			_fontFile="=v:/fonts/Audiowide-Regular.ttf"
			fontFile="=v:/fonts/Video.ttf"
			>
			<gridSize x="32" y="6"/>
			<gridCharacterSize x="166" y="206"/>
			<characterSize x="160" y="200"/>
			<baseLine>52</baseLine>
			<compressCharacters/>
			
			<rowAnsi/>
			<.rowLocalisedCharacters setId="latin"/>
		</texture>
	</generateFontTexture>
	
	<generateFontTexture>
